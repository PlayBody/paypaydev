<script>
    $(function() {
        $('input[id$=_Member]').val('{{ admin_id }}');

        $.ajax({
            url: "{{ url('market_place4_admin_product_class_ex_update') }}",
            cache: false,
            data: {
                'id' : {{ Product.id }},
                'mode' : 'ajax',
                'class_name1_id' : '{{ ClassName1.id }}',
                'class_name2_id' : '{{ ClassName2.id }}'
            },
        }).done(function(data, textStatus, jqXHR){
            $('#div_market_place4_class__append').html(data);
            $('.c-primaryCol>form>div.card').append($('#div_market_place4_class__append'));
        }).fail(function(data, textStatus, errorThrown){
            $('.div_classEx__append').html('取得に失敗しました。');
        });

    });
</script>

<div id="div_market_place4_class__append">

