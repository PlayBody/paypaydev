 <script>
    $(function() {
        $.ajax({
            url: "{{ url('admin_productex_load_class') }}",
            cache: false,
            data: {
                'id' : {{ Product.id }}
            },
        }).done(function(data, textStatus, jqXHR){
            $('#div_classEx__append').html(data);
            $('#standardConfig table').replaceWith($('#div_classEx__append'));
        }).fail(function(data, textStatus, errorThrown){
            $('.div_classEx__append').html('取得に失敗しました。');
        });

    });
</script>

<div id="div_classEx__append">
    読込中・・・
</div>
